{
  "version": "0.2.0",
  "configurations": [
    {
      "timeout": 300000,
      "name": "Serve",
      "type": "node",
      "request": "launch",
      "preLaunchTask": "Serve",
      "program": "${workspaceRoot}/src/service/app.js",
      "stopOnEntry": false,
      "cwd": "${workspaceRoot}",
      "console": "internalConsole",
      "env": {
        "MODE": "dev",
        "HOST": "localhost",
        "PORT": "8090"
      }
    },
    {
      "timeout": 300000,
      "name": "Build",
      "type": "node",
      "request": "launch",
      "preLaunchTask": "Build",
      "program": "${workspaceRoot}/src/service/app.js",
      "stopOnEntry": false,
      "cwd": "${workspaceRoot}",
      "console": "internalConsole",
      "env": {
        "MODE": "prod",
        "HOST": "localhost",
        "PORT": "8090"
      }
    }
  ]
}
